#include <stdio.h>
#include<stdlib.h>
int gcd(int n1,int n2)
{
    if(n2!=0)
        return gcd(n2,n1%n2);
    else return n1;
}
int main(int argc, const char * argv[]) {
    // insert code here...
    int n1,n2,i=1,res=0;
    printf("enter two numbers");
    scanf("%d%d",&n1,&n2);
    n1=abs(n1);
    n2=abs(n2);
    
        //iterative
        while(i<=n1 && i<=n2)
        {
            if(n1%i==0 && n2%i==0)
                {
                    res=i;
                    
                }i++;
        }
printf("%d\n",res);
    
    
        //recursion
        printf("%d",gcd(n1,n2));
        
    
    return 0;
}
