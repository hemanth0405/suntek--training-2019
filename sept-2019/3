#include <stdio.h>
#include<string.h>
int consonants(char a )
{
    if(a!='a'&&a!='e'&&a!='i'&&a!='o'&&a!='u')
        return 1;
    else return 0;
    
}
int main(int argc, const char * argv[]) {
    // insert code here...
    char str1[30],str2[30];
    int i=0,j=0;
    gets(str1);
    for(i=0;strlen(str1)!='\0';i++)
    {
        if(consonants(str1[i]))
        {
            str2[j]=str1[i];
            j++;
        }
    }
    for(i=0;strlen(str1)!='\0';i++)
    {
        if(consonants(str1[i]))
        {
            str1[i]=str2[--j];
        }
    }
    while(str1[i]!='\0')
    {
        printf("%c",str1[i]);
        i++;
    }
    return 0;
}
